class first
{
    public static void main(String[]args)
    {
        int a=20,b=10;
        System.out.println("before swap");
        System.out.println("a="+a);
         System.out.println("b="+b);
        int temp=a;
        a=b;
        b=temp;
       System.out.println("after swap");
       System.out.println("a="+a);
        System.out.println("b="+b);
    }
}

output:
before swap
a=20
b=10
after swap
a=10
b=20
